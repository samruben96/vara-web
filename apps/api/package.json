{
  "name": "@vara/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "prisma generate && tsx watch src/index.ts",
    "build": "prisma generate && tsup",
    "postinstall": "prisma generate",
    "start": "node dist/index.js",
    "worker": "tsx src/workers/index.ts",
    "worker:dev": "tsx watch src/workers/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:push": "prisma db push"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.1.0",
    "@fastify/multipart": "8",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "pg": "^8.13.0",
    "@supabase/supabase-js": "^2.39.0",
    "@vara/shared": "workspace:*",
    "bullmq": "^5.1.0",
    "dotenv": "^17.2.3",
    "fastify": "^4.25.0",
    "ioredis": "^5.3.0",
    "sharp": "^0.34.5",
    "uuid": "^13.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/sharp": "^0.32.0",
    "@types/uuid": "^11.0.0",
    "pino-pretty": "^11.0.0",
    "@types/pg": "^8.11.0",
    "prisma": "^7.3.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
